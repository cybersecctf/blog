    '4': '....-', '5': '.....', '6': '-....', '7': '--...', '8': '---..', '9': '----.', '0': '-----'
}

# Inverted Morse code dictionary for decoding
inverted_morse_code_dict = {value: key for key, value in morse_code_dict.items()}

# American Morse code dictionary
american_morse_code_dict = {
